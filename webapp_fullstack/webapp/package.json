{
  "name": "prompt-scrubber-webapp",
  "version": "1.0.0",
  "description": "SaaS web application for Prompt-Scrubber with subscription management",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:app": "vite build",
    "build": "npm run build:app && npm run copy:netlify",
    "copy:netlify": "cp netlify.toml dist/netlify.toml",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "format": "prettier --write ."
  },
  "dependencies": {
    "date-fns": "^2.30.0",
    "lucide-react": "^0.263.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.8.1",
    "recharts": "^2.7.2",
    "zustand": "^4.3.9"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.6.0",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.27",
    "tailwindcss": "^3.3.3",
    "vite": "^4.5.14",
    "vitest": "^0.34.1"
  }
}
